<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
        // 页面加载完成后发送 AJAX 请求
        $(function() {
            $.ajax({
                url: '/test/index',
                method: 'GET',
                dataType: 'json',
                success: function(data) {
                    // 请求成功时的处理
                    console.log(data)
                    $("#app").text(data.data)
                },
                error: function(xhr, status, error) {
                    // 请求失败时的处理
                    console.log(error);
                }
            });
        });
    </script>
    <title>Header</title>
</head>

<body>
    <div id="app">
        {{message}}
    </div>
</body>

</html>